   "source": [
    "import numpy as np\n",
    "n = 512\n",
    "f = np.random.rand(n)\n",
    "\n",
    "%timeit np.fft.fft(f)\n",
    "\n",
    "k = np.arange(n)\n",
    "m = k.reshape((n,1))\n",
    "w = np.exp(-2j * np.pi * m * k / n)\n",
    "%timeit np.dot(w,f)\n",
    "\n",
    "ftfast = np.fft.fft(f)\n",
    "ftslow = np.dot(w,f)\n",
    "np.allclose(ftfast, ftslow)"
   ]
  }
